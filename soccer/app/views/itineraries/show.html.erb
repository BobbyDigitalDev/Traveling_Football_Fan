

<center>
<h1><%=@user.name%> <%=@itinerary.name%> Itinerary Details</h1>
</center>


<br />

<h2>Football Match Listings</h2>

<section>

<h3>Create New Match</h3>

<select name="stadium_id">
<% @stadia.each do |stadium| %>

<option value='<%= stadium.id %>'><%=stadium.name%></option>

<%end%>
</select> 


<% form_tag '/matches', method: 'post' do %>
<%= text_field_tag :team_1 %> vs <%= text_field_tag :team_2 %>
<%= submit_tag "Create" %>
<%end%>


</section>

<div></div>

